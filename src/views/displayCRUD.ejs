<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Display CRUD</title>
    <style>
        table, th, td {
            border: 1px solid black;
            border-collapse: collapse;
        }
    </style>
</head>
<body>
    <h1>Display CRUD</h1>
    <table style="width: 100%">
        <tr>
            <th>ID</th>
            <th>Email</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Address</th>
            <th>Phone Number</th>
            <th>Gender</th>
            <th>Role ID</th>
        </tr>
        <% for(let i = 0; i < dataTable.length; i++) { %>
            <tr>
                <td><%= dataTable[i].id %></td>
                <td><%= dataTable[i].email %></td>
                <td><%= dataTable[i].firstName %></td>
                <td><%= dataTable[i].lastName %></td>
                <td><%= dataTable[i].address %></td>
                <td><%= dataTable[i].phonenumber %></td>
                <td><%= dataTable[i].gender %></td>
                <td><%= dataTable[i].roleid %></td>
            </tr>
        <% } %>

    </table>
</body>
</html>